public class DeliverGear{
	DeliveryStateEnum state;
	CANTalon Talon;
	//double winchAxis = winchStick.getRawAxis(1);
public DeliverGear(CANTalon deliveryTalon, SmartDashboard dashboard) {
	Talon = deliveryTalon;
}	
	
public void GearCases(CANTalon deliveryTalon , SmartDashboard dashboard){
	switch (state) {
		case ENCODERRESET1:
			
			
			break;
			
		case RAISEDEPLOYER:
			
			deliveryTalon.set(0.5);
			
			break;
			
		case DRIVEFWD:
			
			break;
		
		case LOWERDEPLOYER:
			
			break;
			
		case ENCODERRESET2:
			
			break;
	
	
	}
}
}

